<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("../partials/head")  %>
</head>
<body>
    <h1>Quiz List</h1>

    <%- include("../partials/nav")  %>
    <ul>
        <% for(var i = 0; i < quizes.length; i++){ %>
            <% if( quizes[i].isPublished ) { %>
                <li>
                    <h2>
                        <a href="/<%= quizes[i].slug %>/question/list/"> 
                            <%= quizes[i].title %>  
                        </a>
                    </h2> 
                </li>
    
                <!-- <ul>
                    <li><a href="/<%= quizes[i].slug %>/delete/">remove</a> </li>
                    <li><a href="/<%= quizes[i].slug %>/edit/">edit</a> </li>
                </ul> -->
            <% } %> 
        <% } %>
    </ul>

    <h2>Not published Quizzes.</h2>
    <ul>
        <% for(var i = 0; i < quizes.length; i++){ %>
            <% if( !quizes[i].isPublished ) { %>
                <li>
                    <h2>
                        <a href="/<%= quizes[i].slug %>/question/list/"> 
                            <%= quizes[i].title %>  
                        </a>
                    </h2> 
                </li>
    
                <!-- <ul>
                    <li><a href="/<%- quizes[i].slug %>/delete/">remove</a> </li>
                </ul> -->
            <% } %> 
        <% } %>
    </ul>

    
</body>
</html>


